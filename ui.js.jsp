//---------------------------------AUTOCOMPLETE------------------------------------
//v3
function jaf_autocomplete(tagAutocomplete, minLengthRchiesta, jaf_label, options) {

	if(polij_jslib_version <3){
		return;
	}

	if(!options) {
		options = {};
	}

	var $form = $('#script_autocomplete_'+ tagAutocomplete).parents('form'); // TODO no allegati
	if($form.length == 0) {
		return;
	}

	var $tagAutocompele = jaf_getTagAutocomlete($form);
	if ($tagAutocompele.length == 0) {
		return;
	}

	var urlAction = $form.attr('action');
	var eventName = options.eventName ? options.eventName : "EVN_AUTOCOMPLETE_" + tagAutocomplete;
	if (urlAction !== null && urlAction !== undefined) {
		urlAction += (urlAction.indexOf('?') > 0) ? "&" : "?";
		urlAction += eventName + "=ajax&polij_ajax=true&freezecount=true";
	} else {
		return;
	}

	var rappresentante = new String();
	$tagAutocompele.autocomplete({
		minLength : minLengthRchiesta,
		source : function(request, response) {
			$.ajax({
				url : urlAction,
				dataType : "json",
				type : "post",
				data : datiNellaForm(request, tagAutocomplete, options),
				success : function(data, textStatus, jqXHR) {
					response($.map(data, function(item) {
						return item;
					}));
				},
				error : function(data, textStatus, jqXHR) {
					response($.map(data, function(item) {
						return null;
					}));
				}
			});
		},
		select : function(event, ui) {

			// autocompletamento su campi corelati alla response
			jaf_popolaTagHtmlPassatiFiltrandoAttributo(ui.item, $form.find('[id]'),'id', $form);
			jaf_popolaTagHtmlPassatiFiltrandoAttributo(ui.item, $form.find('[data-jaf-field-name]'),'data-jaf-field-name', $form);
			jaf_popolaTagHtmlPassatiFiltrandoAttributo(ui.item, $form.find('[name]'), 'name', $form);

			// autocompletamento sul campo su sui  associato l'autocomplete
			jaf_popolaHtml_fromTag($tagAutocompele, getValueForTag(ui.item), $form);

			return false;
		}
	});

	// restituisce il valore da usare per l'input da autocompletare
	function getValueForTag(selectedItem) {
		var inputNameValue = $tagAutocompele.attr("data-jaf-field-name") || $tagAutocompele.attr('name');
		var foundMatch = findValue(selectedItem, inputNameValue);

		if(foundMatch) {
			return foundMatch;
		} else {
			var properties = Object.getOwnPropertyNames(selectedItem).filter( function(key){return selectedItem[key];} ).sort();
			if(properties.length == 1) {
				return selectedItem[properties[0]];
			}
			if(jaf_label) {
				var labels = jaf_label.split(",").map(function(l) {return l.trim().toLowerCase();});
				while(labels.length) {
					var label = labels.shift(0);
					var foundValue = findValue(selectedItem, label);
					if(foundValue) {
						return foundValue;
					}
				}
			}
				
		}

		if(selectedItem.hasOwnProperty('value')) {
			return selectedItem.value;
		}

		return '';
	}

	function findValue(list, key) {
		// Ricerca case-insensitive della chiave (restituisce il valore)
		var foundMatch = null;
		$.each(list, function(chiave, valore) {
			// valore e chiave non nulli, chiave corrisponde a inputNameValue (non case-sensitive)
			if(valore && chiave && chiave.toLowerCase() == key.toLowerCase()) {
				foundMatch = valore;
				return false;
			}
		});

		return foundMatch;
	}

	function datiNellaForm(request, tagAutocomplete, options){

		var tadifieldName = {}
		var $fieldName = $form.find('[data-jaf-field-name]');

		$fieldName.each(function(chiave,valore){
			tadifieldName[$(valore).attr('data-jaf-field-name')] = $(valore).val();
		});

		tadifieldName['jaf-search-term'] = request.term;
		tadifieldName['jaf-search-field'] = tagAutocomplete;

		var queryString = $.param(tadifieldName);
		if(queryString.length > 0)
			queryString += "&";

		queryString += $form.serialize();

		return queryString;
	}

	if ($tagAutocompele.data("ui-autocomplete") != null) {
		$tagAutocompele.data("ui-autocomplete")._renderItem = function(ul, item) {
			var rappresentante = '' + getDescriptor(item);
			return $("<li>").append("<a>" + rappresentante + "</a>").appendTo(ul);
		};
	}

	function jaf_getTagAutocomlete($form){
		var $tagAutocompele;

		// per name
		$tagAutocompele = $form.find('[name='+ tagAutocomplete +']');
		if ($tagAutocompele.length > 0) {
			return $tagAutocompele;
		}

		// fomrBean field
		$tagAutocompele = $form.find('[data-jaf-field-name=' + tagAutocomplete + ']');
		if ($tagAutocompele.length > 0) {
			return $tagAutocompele;
		}

		// per id html
		$tagAutocompele = $form.find('#' + tagAutocomplete);
		return $tagAutocompele;
	}

	function getJafLabel(item) {
		var rappresentante = new String();
		var rappresentante_unico = new String();
		var i = 0;

		if (jaf_label == null || jaf_label == undefined) {
			jaf_label = "label";
		}

		$.each(item, function(chiave, valore) {

			if (valore !== null && valore !== undefined  && ("" + valore).length > 0 ) {
				i++;

				if (i == 1){
					rappresentante_unico = valore;
				}

				if (chiave.toLowerCase() == jaf_label.toLowerCase()) {
					rappresentante = valore;
					return false;
				}

				rappresentante += chiave + ': ' + valore + ', ';

			}
		});

		if(i == 1){
			rappresentante = rappresentante_unico;
		}

		return $.trim(rappresentante);
	}

	function getDescriptor(item) {
		if(item.hasOwnProperty('label') && item.label)
			return item.label;

		if (jaf_label == null || jaf_label == undefined) {
			return allKeyValues(item);
		}

		var labels = jaf_label.split(",").map(function(l) {return l.trim().toLowerCase();});

		if( labels.length == 1 ) {
			return keyValuesFromLabels(item, labels, valueOnly);
		} else {
			return keyValuesFromLabels(item, labels, keyValue);
		}

	}

	function valueOnly(k,v) {
		return v;
	}

	function keyValue(k,v) {
		return k + ': '+ v;
	}

	function allKeyValues(item) {
		// non undefined properties
		var properties = Object.getOwnPropertyNames(item).filter( function(key){return item[key];} );
		if(properties && properties.length == 1) {
			return item[properties[0]];
		}

		var rappresentanti = [];
		$.each(item, function(chiave, valore) {
			if(valore) {
				rappresentanti.push(chiave + ': ' + valore);
			}

		});
		
		return rappresentanti.join(", ");
	}

	function keyValuesFromLabels(item, labels, consumer) {
		var rappresentanti = [];

		labels.forEach(function(jafLabel) {

			$.each(item, function(chiave, valore) {
				if ( valore && chiave.toLowerCase() === jafLabel ) {
					if(typeof consumer === 'function') {
						rappresentanti.push( consumer(chiave, valore) );
					} else {
						rappresentanti.push(chiave + ': ' + valore);
					}
				}
			});

		});

		return rappresentanti.join(", ");
	}
}
