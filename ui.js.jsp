//		$(idTemp + ' input[polij_tabella_filtri]').keyup (function(e) {
		$(idTemp + ' input[polij_tabella_filtri]', idTemp + ' input[data-polij-tabella-filtri]').keyup (function(e) {
			// verifico se  server Side
			if($(this).closest("div[data-table-serverSide]").length == 0)
				return;

			var formTimer = null;
			var key_code = e.keyCode ? e.keyCode : e.which;

			if(key_code == 13){
				e.preventDefault();
				return false;
			}

			if(key_code >90 || key_code < 46 )
				if( key_code!=8 && key_code!=32 && !(key_code >95 && key_code <112))
					return;

			var element = this;
			jaf_tabella_asincrona_filtriContatore = 1 + jaf_tabella_asincrona_filtriContatore;
			var filtroContatoreAttuale = jaf_tabella_asincrona_filtriContatore;
			formTimer = window.setTimeout(function(){
				inviaRichiestaFiltri(element, filtroContatoreAttuale);
			}, 600);
//			return false;
		});

		$(
			idTemp + ' input[type="submit"][name="evn_ricerca_recordset=evento"],' +
			idTemp + ' button[type="submit"][name="evn_ricerca_recordset=evento"]'
		).click (function(e) {
			// verifico se  server Side
			if($(this).closest("div[data-table-serverSide]").length == 0)
				return;

			e.preventDefault();
			jaf_tabella_asincrona_filtri_click(this);
			return false;
		});
	}
	catch(exc){console.log(exc);}
}

