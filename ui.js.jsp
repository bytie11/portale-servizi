function jaf_popUp_open(e, tag){
	if (null!=e)
		e.preventDefault();
	
	//console.log("jaf_popUp_open(", e, ",", tag, ")");
	
	var $this = $(tag);
	var $href = $this.attr('href');

	var popUpHeight    = jaf_verificaPresenzaDiUnAttr(tag, 'data-jaf-popup-height', $(window).height()-90);
	var popUpWidth     = jaf_verificaPresenzaDiUnAttr(tag, 'data-jaf-popup-width',  $(window).width()-180);
	var popUpResizable = jaf_verificaPresenzaDiUnAttr(tag, 'data-jaf-popup-resizable', 'false')=='true';

	var popUpCloseUrl = jaf_verificaPresenzaDiUnAttr(tag, 'data-jaf-popup-close-url', '');
	var popUpCodeJs = jaf_verificaPresenzaDiUnAttr(tag, 'data-jaf-popup-code-js', '');
	var popUpNavigate = jaf_verificaPresenzaDiUnAttr(tag, 'data-jaf-popup-navigate', 'false')=='true';

	popUpHeight    = jaf_verificaPresenzaDiUnAttr(tag, 'data-popup-height', popUpHeight);
	popUpWidth     = jaf_verificaPresenzaDiUnAttr(tag, 'data-popup-width',  popUpWidth);
	popUpResizable = jaf_verificaPresenzaDiUnAttr(tag, 'data-popup-resizable', ''+popUpResizable)=='true';
	
	var loaderHtml = "<div id='jaf_popupdataload'>La pagina &egrave; in caricamento...<img src='//webcommons.polimi.it/webcommons/assets/loading.gif'></div>";
	$('<div id="jaf_popupdiv">' + loaderHtml + '</div>').dialog({

		title: ($this.attr('title')) ? $this.attr('title') : 'Dettaglio',
		autoOpen: true,
		modal: true,
		height: popUpHeight, 
		width: popUpWidth, 
		resizable: popUpResizable,
		autoResize: false,
		position: {my: 'center', at: 'center', of: window},
		overlay: {
	    	opacity: 0.5,
	    	background: 'black'
		},

		//================================ open popUp
		open: function(event, ui) {
			// nascondi pulsante chiusura
			if(popUpCloseUrl=="hidden")
				$(this).parent().children().children('.ui-dialog-titlebar-close').hide();
			
			// carica iframe
			$(this).append("<iframe id='jaf_popupiframe' class='externalSite' src='' scrolling='auto' frameborder='0' width='100%' height='98%'/>"); //Crea il tag iframe
			
			// per retroCompatibilit navigablile
			var dest = $href;
			if(dest.indexOf('jaf_ajax=')<=0){
				dest += (dest.indexOf('?')>=0) ? '&' : '?';
				dest += 'jaf_ajax=true';
			}
			$('#jaf_popupiframe').attr('src', dest );
		},
		//================================ fine open popUp
		
		//================================ pre chiusura popUp
		beforeclose: function (e, ui) 
		{
			// js popUpCodeJs indica il nome di una funzione js
			if(''!=popUpCodeJs){
				var $iframe =$('iframe#jaf_popupiframe');
				var domPopUp = null;
				
				try {
					domPopUp = $iframe.contents().find('html');
					$.globalEval('var f =' + popUpCodeJs + '(e, ui, domPopUp, $href);');
					callback = f;
	            } catch(error) {
//	                console.log(error);
	                return true;
	            }
	            
	            if(callback===false)
					return false;
			}
			return true;
		},
		//================================ fine pre chiusura popUp
		
		
		//================================ fine chiusura popUp
		close: function(event, ui) {
			$(this).attr('src', '');
			$(this).dialog('destroy').remove();
			if(""!=popUpCloseUrl && "hidden"!=popUpCloseUrl)
			{
				window.location = popUpCloseUrl; 
			}
		}
		//================================ fine chiusura popUp
		
	});
	
	// rimuove animazione del caricamento
	$('#jaf_popupiframe').on('load',function(){
		$('#jaf_popupdiv #jaf_popupdataload').remove();
		
		try{

			var $iframe = $('iframe#jaf_popupiframe');
			var $htmlIframePopUp = $iframe.contents().find('html');
	
			// target
			if(!popUpNavigate){
				$htmlIframePopUp.find('a, form').attr('target', '_top');
			}
			
			// style
			$htmlIframePopUp.find('head').append("<style>#poliheader, #policontext, #infoDB, #polifooter, .polifooter, .LeftBar{ display: none;}</style>");
	
			// exit popUp
			$htmlIframePopUp.find('*[data-jaf-popUp-close]').attr('target', '_top');
			$htmlIframePopUp.find('.popUpClose').attr('target', '_top');
		} catch(error) {
//	         console.log(error);
			return;
		}
	});

	//	TODO da mettere in css
	$('iframe#jaf_popupiframe').width('98%');  
}

