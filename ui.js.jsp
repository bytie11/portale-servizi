//*********** core per funzianalita asincrone della tabella **************//
function inviaRichiestaFiltri(i_input_tag, filtroContatoreAttuale) {
	if(filtroContatoreAttuale < jaf_tabella_asincrona_filtriContatore)
		return false;

	var input_tag = $(i_input_tag);

	var id_div_table = "div_table_" + input_tag.attr('data-polij-tabella-filtri');

	// da toglere dopo il rilascio ufficiale
	if(input_tag.attr('data-polij-tabella-filtri') === undefined)
		var id_div_table = "div_table_" + input_tag.attr('polij_tabella_filtri');

	var parent_form = $(input_tag.closest("form"));
	var url_combinato = getUrlForm(input_tag);

	gestioneChiamataAsincrona(url_combinato, id_div_table, parent_form);
}

function getUrlForm(input_tag){
	var parent_form = $(input_tag.closest("form"));
	var url_combinato = parent_form.attr('action');
	url_combinato += (url_combinato.indexOf("?") > 0) ? "&" : "?";
	url_combinato += "evn_ricerca_recordset=evento";
	return url_combinato;
}

function createTempContainer(text)
{
   text = text.replace(/^.*<\s*body(>|\s[^>]*>)/gi, '').replace(/<\/\s*body\s*>.*$/gi, '');
   
   return $("<div>" + text + "</div>");
}


function gestioneChiamataAsincrona(url_combinato, id_div_table, parent_form){
	$.ajax({
		type : "post",
		url : '' + url_combinato,
		data : parent_form.serialize(),
		success : function(result) {
			var oldTable = $('#' + id_div_table + " table.TableDati").eq(0);
			var oldTbody = $('#'+id_div_table+' table .TableDati-tbody').eq(0);
			var oldElencoCampi = $('#'+id_div_table+' .elenco-campi');
			var oldPaginator = $('#'+id_div_table+' .paginator');

			// se il POST ritorna una pagina intera invece di un frammento, eliminiamo tutto
			// quello che viene prima e dopo il body (compresi i tag che lo delimitano),
			// in modo che non succedano cose strane durante la manipolazione successiva.
			var div_temp = createTempContainer(result);

			var newTbody = div_temp.find('#' + id_div_table + " table .TableDati-tbody").eq(0);
			var newElencoCampi = $('#'+id_div_table+' .elenco-campi');
			var newPaginator = $(div_temp.find('#' + id_div_table + " .paginator")).eq(0);

			oldElencoCampi.html(newElencoCampi.html());
			oldTbody.html(newTbody.html());

			if(newPaginator.length>0)
				oldPaginator.html(newPaginator.html());
			else
				oldPaginator.html("");

			jaf_tabella_asincrona_ordinamento(id_div_table);
			jaf_tabella_asincrona_paginator(id_div_table);

			oldTable.trigger("jaf_event_tabella_onLoad");
		}
	});
}

/** 
 *    Versione "light" del macchinario ajax per versione iper-compatibile con vecchio recordset:
 *    gestiamo solo gli eventi di ordinamento via GET
 */

function jaf_tabella_ordinamento_ajax(table_id) {

   try
   {
      var $target = $('table#' + table_id).closest('div.div-content-table');
      if ($target.length < 1) 
      {
         console.log("Errore: fallita inizializzazione tabella con ordinamento ajax");
      }
      
      var target_id = $target.attr('id');

      function ec_click(event)
      {
         event.preventDefault();

         var url = $(this).attr('href');
         if (!url) return false;
         
         // sostituiamo il contenuto della div esterna con quello preso dalla pagina postata
         
         $target.load(url + ' #' + target_id + ' > *', function() {
            
            // facciamo scattare l'evento di onload a beneficio del contenuto della tabella
            $target.find('table#' + table_id).trigger("jaf_event_tabella_onload");
            
         });
         return false;
      }
      
      
      // use a delegated event handler so we don't need to put it back in place
      // after every reload
      if ($target.on)
      {
         $target.on('click', 'tr.elenco-campi a', ec_click);
      }
      else
      {
         $target.delegate('tr.elenco-campi a', 'click', ec_click);
      }
      
   }
   catch(e)
   {
     console.log(e.message);
   }

}


