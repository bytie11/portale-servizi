// 20150604 andrea
// utilizziamo un event handler delegato per evitare di "perdere" gli handler agganciati a link popup
// all'interno di elementi ricaricati via ajax (es. paginatore server-side)

$(document).ready(function() {
   
   function jpo(e)
   {
      jaf_popUp_open(e, this);
      return false; 
   }

   var $doc = $(document);
   
   // Evitiamo di ri-inizializzare per errore i listener per i popup
   if($doc.data('jaf-popup-listeners-initialized')) {
		return;
   }

   // jQuery vecchi (jslibs = v1) non hanno $().on
   if ($doc.on)
   {
      $doc.on('click', 'a[data-jaf-popup-to=true]', jpo);
      $doc.data('jaf-popup-listeners-initialized', true);
   }
   else
   {
      $doc.delegate('a[data-jaf-popup-to=true]', 'click', jpo);
   }
   
});

