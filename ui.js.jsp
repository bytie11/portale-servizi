var jafActions = {
	RedirectAction: function(target, recipient, options) {
		this.target = target;
		this.recipient = recipient;
		this.options = options;

		this.trigger = function() {
			var targetUrl = this.target || this.recipient;
			if(targetUrl && targetUrl.length) {
				window.location.assign(targetUrl);
			}
		}
	},

	/*
	 * FormSubmitAction.trigger() supporta le opzioni:
	 * -> skipValidation (Boolean): salta la validazione della form
	 */
	FormSubmitAction: function(target, recipient, options) {
		if(typeof target === 'string') {
			this.target = $(target);
		} else {
			this.target = target;
		}
		this.recipient = recipient;


		// Aggiungo alle opzioni predefinite quelle passate come terzo parametro
		this.options = $.extend( {
			skipValidation: false
		}, options || {});

		this.trigger = function(userParams) {
			this.defaultParams = this.options;

			var params = {};
			if(typeof userParams === 'undefined') {
				params = this.defaultParams;
			} else {
				$.extend(params, this.defaultParams, userParams);
			}

			var $form = this.target;
			if($form && $form.length == 1) {
				this._prepare(this.recipient, params);

				var validator = $form.data('validator');
				var hasValidator = validator != null || true;

				if(hasValidator && params.skipValidation) {
					$form.get(0).submit(); // Usa la submit html per evitare le validazioni di jQuery.Validation
				} else {
					$form.submit();
				}
			}
		}
		
		this._prepare = function(actionTarget, params) {
			var $form = this.target;

			if(typeof actionTarget !== 'undefined') {
				var $input = $("<input />");
				$input.attr("type","hidden");
				$input.attr("name",actionTarget);
				$input.attr("value","evento");

				$form.append($input);
			}

			if(params.freezecount) {
				$form.append($('<input type="hidden" name="freezecount" value="true" />'));
			}
		}
	},
	CustomAction: function(target, recipient, options) {
		this.target = target;
		this.recipient = recipient;
		this.options = options;

		this.trigger = function() {
			var functionName = this.target || this.recipient;
			if(window.hasOwnProperty(functionName) && window[functionName] instanceof Function) {
				var params = {
					element: this.options.element
				};
				window[functionName].call(window,params);
			}
		}
	}
	
}

