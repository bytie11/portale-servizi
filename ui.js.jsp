				return;
			}
			logger.trace("Inizializzazione calendario ["+this.fieldKey+"] ...");
			logger.trace(this.options);
			logger.trace(this.settings);

			if(!this._isReadOnly()) {
				this._initWidget();
			}

			this._initFormatInput();
			this._initActions(this.settings.action);
			this.ready = true;
			logger.trace("Calendario pronto: "+this.fieldKey);
